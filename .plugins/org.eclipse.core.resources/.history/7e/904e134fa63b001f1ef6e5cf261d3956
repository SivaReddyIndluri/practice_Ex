package strings;

public class ExceptionClass {
	public static void main(String[] args) {
		try {
			int a =10/0;
			System.out.println("This will not printed:");
		}
		catch (Exception e) {
			System.out.println("Exception caught:");
		}
		finally {
			System.out.println("Final block  will get executed:");
		}
		
	}
}
