package arrayExamples;

public class ArrayEx {
	
	public void m1() {
		
		int[] arr = {12,54,2,87,4,23};
		int ns = 100;
		for(int i =0; i<=arr.length-1;i++) {
			if(arr[i]==ns) {
				System.out.println("given number is found:" +arr[i]);
				break;
			}
		}
		
	}
	
	public static void main(String[] args) {
		ArrayEx ex = new ArrayEx();
		ex.m1();
		
	}

}
